model Subscriptions {
    id          String   @id @default(uuid())
    name        String   @unique
    price       Float    @default(0)
    validity    String?
    description String?
    active_flag Boolean  @default(true)
    delete_flag Boolean  @default(false)
    modified_at DateTime @updatedAt
    created_at  DateTime @default(now())
    created_by  String?
    modified_by String?
}
