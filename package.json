  "scripts": {
    "dev": "nodemon",
    "prod": "nodemon",
    "prisma:generate": "prisma generate --schema=./src/infrastructure/entities/schema",
    "prisma:migrate:dev": "prisma migrate dev --schema=./src/infrastructure/entities/schema",
    "prisma:migrate:prod": "prisma migrate deploy --schema=./src/infrastructure/entities/schema",
    "prisma:migrate:reset": "prisma migrate reset --schema=./src/infrastructure/entities/schema",
    "clear-db": "rm -rf ./src/infrastructure/entities/schema/migrations && npm run prisma:migrate:dev"
  },
